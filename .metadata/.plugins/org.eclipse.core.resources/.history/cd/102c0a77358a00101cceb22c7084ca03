package com.shoutbreak.service;

public class UserEvent {
	
	private User _source;
	private UserEventType _type;
	public static enum UserEventType { densityChange };
	
	public UserEvent(User source, UserEventType type) {
		_source = source;
		_type = type;
	}

	public User getSource() {
		return _source;
	}
	
}
