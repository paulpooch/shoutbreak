<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="co.shoutbreak" android:versionCode="2" android:versionName="2.0">
    <uses-sdk android:minSdkVersion="8" />
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".ui.ShoutbreakUI" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".service.SBService" android:exported="true">
			<intent-filter>
				<action android:name="co.shoutbreak.service.SBService" />
			</intent-filter>
		</service>
		<receiver android:name="service.SBAlarmReceiver" android:exported="true">
			<intent-filter>
				<action android:name="co.shoutbreak.service.SBAlarmReceiver" />
			</intent-filter>
		</receiver>
    </application>
</manifest>